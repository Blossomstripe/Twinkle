{
	"title": "Twinkle",
	"name": "qiuwen-twinkle",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://git.qiuwen.wiki/InterfaceAdmin/Twinkle"
	},
	"scripts": {
		"deploy": "pnpm babel && node scripts/deploy.mjs",
		"babel": "pnpm format && babel -d dist src",
		"tsc": "tsc",
		"format": "prettier --write . && pnpm run lint:fix",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"stylelint": "stylelint src/*.css",
		"stylelint:fix": "stylelint src/*.css --fix",
		"lint": "eslint src && stylelint src/*.css",
		"lint:fix": "pnpm run eslint:fix && pnpm run stylelint:fix",
		"updatebrowserslist": "pnpx update-browserslist-db@latest"
	},
	"keywords": [
		"Qiuwen",
		"MediaWiki"
	],
	"devDependencies": {
		"@babel/cli": "^7.21.5",
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@types/jqueryui": "^1.12.16",
		"@types/oojs-ui": "^0.46.1",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"babel-plugin-array-includes": "^2.0.3",
		"chalk": "^5.2.0",
		"core-js": "^3.30.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-wikimedia": "^0.25.0",
		"git-commit-msg-linter": "^5.0.4",
		"minimist": "^1.2.8",
		"mwn": "^1.11.5",
		"prettier": "^2.8.8",
		"prompts": "^2.4.2",
		"stylelint": "^15.6.1",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"types-mediawiki": "^1.3.0",
		"typescript": "^5.0.4"
	}
}
